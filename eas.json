{
  "cli": {
    "version": ">= 3.0.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "SENTRY_ALLOW_FAILURE": "true"
      },
      "ios": {
        "resourceClass": "m-medium",
        "simulator": false,
        "buildConfiguration": "Debug"
      },
      "android": {
        "resourceClass": "medium"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "SENTRY_ALLOW_FAILURE": "true"
      },
      "ios": {
        "resourceClass": "m-medium",
        "simulator": false,
        "buildConfiguration": "Release"
      },
      "android": {
        "resourceClass": "medium",
        "buildType": "apk"
      }
    },
    "production": {
      "env": {
        "SENTRY_ALLOW_FAILURE": "true"
      },
      "ios": {
        "resourceClass": "m-medium",
        "buildConfiguration": "Release",
        "autoIncrement": true
      },
      "android": {
        "resourceClass": "medium",
        "buildType": "app-bundle",
        "autoIncrement": true
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "appleId": "nour@maaktech.net",
        "ascAppId": "6758102532",
        "appleTeamId": "4B9GHD4482"
      },
      "android": {
        "serviceAccountKeyPath": "./google-service-account.json",
        "track": "internal"
      }
    }
  }
}
