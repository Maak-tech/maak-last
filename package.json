{
  "name": "maak-health-app",
  "main": "index.js",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "node scripts/start-web.js",
    "dev": "expo start --dev-client",
    "dev:tunnel": "expo start --dev-client --tunnel",
    "dev:clear": "expo start --dev-client --clear",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "lint": "biome check .",
    "build": "npm run build:web",
    "build:web": "expo export --platform web",
    "build:ios:dev": "eas build -p ios --profile development",
    "build:android:dev": "eas build -p android --profile development",
    "build:dev": "eas build --profile development --platform all",
    "build:ios:preview": "eas build -p ios --profile preview",
    "build:android:preview": "eas build -p android --profile preview",
    "build:ios:production": "eas build -p ios --profile production",
    "build:android:production": "eas build -p android --profile production",
    "build:list": "eas build:list",
    "firebase:init": "npx tsx scripts/firebase-init-collections.ts",
    "firebase:check": "npx tsx scripts/firebase-setup-check.ts",
    "firebase:create-collections": "npx tsx scripts/create-user-collections.ts",
    "firebase:fix-medications": "node scripts/fix-medications.js",
    "validate:env": "npx tsx scripts/validate-env.ts",
    "validate:production": "npx tsx scripts/validate-production.ts",
    "verify:secrets": "npx tsx scripts/verify-secrets.ts",
    "check:zeina": "npx tsx scripts/check-zeina-config.ts",
    "check:openai": "npx tsx scripts/check-openai-key.ts",
    "test:websocket": "npx tsx scripts/test-websocket.ts",
    "test:ppg": "npx tsx scripts/test-ppg.ts",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "postinstall": "node -e \"const fs=require('node:fs');const {spawnSync}=require('node:child_process');const patchDir='patches';if(!fs.existsSync(patchDir)||fs.readdirSync(patchDir).filter((file)=>file.endsWith('.patch')).length===0){console.log('No patch-package patches found; skipping.');process.exit(0);}let patchCli;try{patchCli=require.resolve('patch-package/dist/index.js');}catch(error){if(error&&error.code==='MODULE_NOT_FOUND'){console.log('patch-package not installed; skipping postinstall patches.');process.exit(0);}throw error;}console.log('Applying patch-package patches...');const result=spawnSync(process.execPath,[patchCli],{stdio:'inherit'});if(result.status!==0){process.exit(result.status??1);}\""
  },
  "dependencies": {
    "@expo-google-fonts/cairo": "^0.4.2",
    "@expo-google-fonts/inter": "^0.2.3",
    "@expo-google-fonts/noto-sans-arabic": "^0.4.3",
