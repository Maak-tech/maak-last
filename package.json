{
  "name": "maak-health-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "expo start --dev-client",
    "dev:tunnel": "expo start --dev-client --tunnel",
    "dev:clear": "expo start --dev-client --clear",
    "build:web": "expo export --platform web",
    "build:ios:dev": "eas build -p ios --profile development",
    "build:android:dev": "eas build -p android --profile development",
    "build:dev": "eas build --profile development --platform all",
    "build:ios:preview": "eas build -p ios --profile preview",
    "build:android:preview": "eas build -p android --profile preview",
    "build:ios:production": "eas build -p ios --profile production",
    "build:android:production": "eas build -p android --profile production",
    "build:list": "eas build:list",
    "firebase:init": "bunx tsx scripts/firebase-init-collections.ts",
    "firebase:check": "bunx tsx scripts/firebase-setup-check.ts",
    "firebase:create-collections": "bunx tsx scripts/create-user-collections.ts",
    "firebase:fix-medications": "node scripts/fix-medications.js",
    "validate:env": "bunx tsx scripts/validate-env.ts",
    "validate:production": "bunx tsx scripts/validate-production.ts",
    "verify:secrets": "bunx tsx scripts/verify-secrets.ts",
    "check:zeina": "bunx tsx scripts/check-zeina-config.ts",
    "check:openai": "bunx tsx scripts/check-openai-key.ts",
    "test:websocket": "bunx tsx scripts/test-websocket.ts",
    "test:ppg": "bunx tsx scripts/test-ppg.ts",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "postinstall": "node -e \"const { execSync } = require('node:child_process'); try { const patchCli = require.resolve('patch-package/dist/index.js'); execSync(`node \\\"${patchCli}\\\"`, { stdio: 'inherit' }); } catch (error) { if (error && error.code === 'MODULE_NOT_FOUND') { console.log('patch-package not installed; skipping postinstall patches.'); process.exit(0); } throw error; }\""
  },
  "dependencies": {
    "@expo-google-fonts/cairo": "^0.2.3",
    "@expo-google-fonts/inter": "^0.2.3",
    "@expo/metro": "^54.2.0",
    "@expo/metro-config": "^54.0.14",
    "@expo/metro-runtime": "^6.1.2",
    "@expo/vector-icons": "^15.0.3",
    "@kingstinct/react-native-healthkit": "^13.0.2",
