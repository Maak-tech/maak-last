# Copy to `.env` or set via EAS secrets. Never commit real secrets.
#
# Firebase (required for real production; demo fallbacks exist in some scripts)
EXPO_PUBLIC_FIREBASE_API_KEY=
EXPO_PUBLIC_FIREBASE_AUTH_DOMAIN=
EXPO_PUBLIC_FIREBASE_PROJECT_ID=
EXPO_PUBLIC_FIREBASE_STORAGE_BUCKET=
EXPO_PUBLIC_FIREBASE_MESSAGING_SENDER_ID=
EXPO_PUBLIC_FIREBASE_APP_ID=
EXPO_PUBLIC_FIREBASE_MEASUREMENT_ID=

# Optional integrations
# OpenAI keys are BACKEND-ONLY: set them as Firebase Functions secrets (recommended),
# or in `functions/.env` for local emulators. Do NOT expose them via EXPO_PUBLIC_*.
OPENAI_API_KEY=
ZEINA_API_KEY=
FITBIT_CLIENT_ID=
FITBIT_CLIENT_SECRET=

# RevenueCat (public SDK keys used by the mobile client)
REVENUECAT_PROJECT_ID=
PUBLIC_REVENUECAT_IOS_API_KEY=
PUBLIC_REVENUECAT_ANDROID_API_KEY=
REVENUECAT_ENTITLEMENT_ID=
REVENUECAT_OFFERING_ID=

# OAuth Redirect URIs - Must be HTTPS
# Dexcom Redirect URI - Must be HTTPS (e.g., https://yourdomain.com/dexcom-callback)
DEXCOM_REDIRECT_URI=
# Garmin Redirect URI - Must be HTTPS (e.g., https://yourdomain.com/garmin-callback)
GARMIN_REDIRECT_URI=
# Oura Redirect URI - Must be HTTPS (e.g., https://yourdomain.com/oura-callback)
OURA_REDIRECT_URI=
# Samsung Health Redirect URI - Must be HTTPS (e.g., https://yourdomain.com/samsung-health-callback)
SAMSUNG_HEALTH_REDIRECT_URI=

# CI / tooling
SENTRY_AUTH_TOKEN=
